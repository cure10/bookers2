<%= render 'books/profile.new', book: @book, user: @user %>
 <div class="col-lg-9">
   <table>
     <h1>Users</h1>
      <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
        <td></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show',user_path(user.id) %></td>
        </tr>
        <% end %>
        <%= paginate @users %>
      </tbody>
   </table>
  </div>
</div>